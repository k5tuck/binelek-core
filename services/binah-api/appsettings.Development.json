{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    }
  },

  "Services": {
    "Ontology": {
      "Url": "http://ontology:8091",
      "Timeout": 30
    },
    "Context": {
      "Url": "http://context:8096",
      "Timeout": 30
    },
    "Auth": {
      "Url": "http://auth:8093",
      "Timeout": 30
    }
  },

  "RateLimit": {
    "RequestsPerMinute": 1000
  }
}
