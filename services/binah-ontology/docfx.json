{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "src": "."
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "properties": {
        "TargetFramework": "net8.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/**.yml", "api/index.md" ]
      },
      {
        "files": [ "README.md", "toc.yml", "index.md" ]
      }
    ],
    "resource": [
      {
        "files": [ "images/**" ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appName": "Binah Ontology API",
      "_appTitle": "Binah Ontology Service - API Documentation",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/k5tuck/binelek-core",
        "branch": "main"
      }
    },
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
